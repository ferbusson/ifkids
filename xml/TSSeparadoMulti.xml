   16 <!-- Dias de credito -->
   <package validate="true">
      <field row="last" textLine="DIAS DE CREDITO" fontName="Dialog" fontSize="10" fontStyle="0" rowAcum="15" textCol="13"  col="180" type="STRING"/>
   </package>

   17 <!-- Vencimiento -->
