      <field row="last"  textLine="VENCIMIENTO" fontName="Dialog" fontSize="12" fontStyle="0" rowAcum="15" textCol="13"  col="140"  type="STRING"/>
   </package>
   18 <!-- Linea -->
   <package>
      <line row="last" rowAcum="20" col="2" row2="last" col2="200" />
   </package>  
  19 <!-- TRM -->
   <package>
